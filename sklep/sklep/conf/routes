GET    /api/user                        controllers.UserController.getAll()
POST   /api/user                        controllers.UserController.create()
GET    /api/user/$id<[1-9][0-9]*>       controllers.UserController.getById(id: Long)
PUT    /api/user/$id<[1-9][0-9]*>       controllers.UserController.modifyById(id: Long)
DELETE /api/user/$id<[1-9][0-9]*>       controllers.UserController.deleteById(id: Long)

